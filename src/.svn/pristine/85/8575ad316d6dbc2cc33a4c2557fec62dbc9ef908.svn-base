package Testing;

import java.io.IOException;
import java.io.OutputStream;

public class generateChars {

    public static void printChars(OutputStream out) throws IOException {

        int firstPrintableChar = 33;
        int numberOfPrintableChar = 94;
        int numberOfCharsPerLine = 72;

        int start = firstPrintableChar;

        while (true) {

            for (int i = start; i < start + numberOfCharsPerLine; i++) {
                out.write((
                        (i - firstPrintableChar) % numberOfPrintableChar)
                        + firstPrintableChar);
            }
            out.write('\r');
            out.write('\n');

            start = ((start + 1) - firstPrintableChar)
                    % numberOfPrintableChar + firstPrintableChar;
        }

    }


}
